<?xml version="1.0" encoding="UTF-8" ?>

<widget xmlns				= "http://www.w3.org/ns/widgets"
		xmlns:gap			= "http://phonegap.com/ns/1.0"
		id					= "com.mariabultena.TrafficCam"
		versionCode			= "1"
		version  			= "1.0.0" >

	<name>Local Traffic Cams</name>

	<description>See updated stills from traffic cameras in the Tri-Cities.</description>

	<author href = "http:github.com/mariabultena/Traffic-Cams" email = "mariabultena@gmail.com"> Maria Bultena </author>

	<plugin name="cordova-plugin-whitelist" spec="~1.3.2" />
	
	<icon src = "ic_launcher.png" 
      	gap:platform = "android" gap:qualifier = "ldpi" />
		
  	<icon src = "ic_launcher.png" 
      	gap:platform = "android" gap:qualifier = "mdpi" />
		
   	<icon src = "ic_launcherHDPI.png" 
      	gap:platform = "android" gap:qualifier = "hdpi" />
		
   	<icon src = "ic_launcherXHDPI.png" 
      	gap:platform = "android" gap:qualifier = "xhdpi" />
		
   	<icon src = "ic_launcherXXHDPI.png" 
      	gap:platform = "android" gap:qualifier = "xxhdpi" />

   	<icon src = "ic_launcherXXXHDPI.png" 
      	gap:platform = "android" gap:qualifier = "xxhdpi" />




